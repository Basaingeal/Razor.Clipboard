{
  "name": "currietechnologies-razorclipboard",
  "version": "1.4.0",
  "description": "A Razor Class Library for interacting with the browser clipboard",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack --color --mode=production",
    "build:tsc": "tsc",
    "watch": "webpack --watch",
    "lint": "eslint ./src --ext .ts",
    "prettier:write": "prettier --write ./src/*.ts",
    "prettier:check": "prettier --check ./src/*.ts",
    "babel": "babel ./src/clipboard.ts --out-dir wwwroot --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": [
    "chrome 66",
    "edge 79",
    "firefox 63",
    "opera 53",
    "safari 13.1",
    "and_chr 66",
    "and_ff 63",
    "ios_saf 13.4",
    "samsung 9.2"
  ],
  "author": "Michael J. Currie"
}
