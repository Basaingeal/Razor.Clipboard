{
  "name": "currietechnologies-razorclipboard",
  "version": "1.4.0",
  "description": "A Razor Class Library for interacting with the browser clipboard",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack --color --mode=production",
    "build:tsc": "tsc",
    "watch": "webpack --watch",
    "lint": "eslint ./src --ext .ts",
    "prettier:write": "prettier --write ./src/*.ts",
    "prettier:check": "prettier --check ./src/*.ts",
    "babel": "babel ./src/clipboard.ts --out-dir wwwroot --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.5",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "browserslist": [
    "chrome 66",
    "edge 79",
    "firefox 63",
    "opera 53",
    "safari 13.1",
    "and_chr 66",
    "and_ff 63",
    "ios_saf 13.4",
    "samsung 9.2"
  ],
  "author": "Michael J. Currie"
}
